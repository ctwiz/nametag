<h1>Domains you're following</h1>
  <table id="domains">
    <th>
      <tr>
      <td>Name</td><td>Confirmed</td><td>Public</td><td>Available</td><td>Price</td>
      </tr>
    </th>
    <tbody>
      <% @domains_following.each do |dom| %>
        <% dom = dom.domain %>
        <tr>
          <td><%= link_to domain_path(dom.id) do %><%=dom.name%><% end %></td>
          <td><%=dom.confirmed%></td>  
          <td><%=dom.public%></td>
          <td><%=dom.for_sell%></td>
          <td><%=dom.price%></td>
        </tr>
      <% end %>
    </tbody>
  </table>

